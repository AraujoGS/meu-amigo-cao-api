{
  "name": "meu-amigo-cao-api",
  "version": "1.0.0",
  "description": "Projeto para praticar boas práticas de programação",
  "main": "dist/main/server.js",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "start": "node --require dotenv/config dist/main/server.js",
    "test": "jest --passWithNoTests --no-cache --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:ci": "npm test -- --coverage",
    "test:staged": "npm test -- --findRelatedTests ./tests",
    "prepare": "husky install"
  },
  "author": "Guilherme AS",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^16.11.11",
    "@types/pg": "^8.6.1",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.3",
    "@types/validator": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "faker": "^5.5.3",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "module-alias": "^2.2.2",
    "pg-mem": "^2.1.9",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "supertest": "^6.1.6",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
  }
}
